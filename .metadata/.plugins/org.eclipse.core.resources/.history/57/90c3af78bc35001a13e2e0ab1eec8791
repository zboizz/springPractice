<mapper namespace="com.cos.damain.lecture">
	<insert id="insertDao">
	Insert into user(userID, userPW, userName, userEmail) values(#{id}. #{pwd}, #{name}. #[email])
	</insert>
	<select id="listDao" resultType="com.cos.domain.MemberVO">
	select userID, userPW, userNAME, userEmail from user where userID = #{id}
	</select>
</mapper>